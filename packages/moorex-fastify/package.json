{
  "name": "@moora/moorex-fastify",
  "version": "0.1.0",
  "description": "Fastify plugin for Moorex - exposes Moorex state and signals via HTTP API with SSE support",
  "keywords": [
    "moorex",
    "fastify",
    "sse",
    "server-sent-events",
    "state-machine",
    "api",
    "plugin"
  ],
  "main": "./dist/index.js",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build",
    "test": "vitest --run --no-ui",
    "test:coverage": "vitest --run --coverage --no-ui",
    "test:ui": "vitest --ui",
    "test:ci": "vitest --run --coverage --no-ui"
  },
  "files": [
    "dist",
    "README.md",
    "README.zh.md"
  ],
  "dependencies": {
    "@moora/moorex": "0.1.0",
    "fastify": "^5.1.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.12",
    "@vitest/ui": "4.0.12",
    "vitest": "^4.0.12"
  },
  "peerDependencies": {
    "typescript": "^5",
    "mutative": "^1.3.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shazhou-ww/moora.git",
    "directory": "packages/moorex-fastify"
  },
  "homepage": "https://github.com/shazhou-ww/moora/tree/main/packages/moorex-fastify#readme",
  "bugs": {
    "url": "https://github.com/shazhou-ww/moora/issues"
  }
}
