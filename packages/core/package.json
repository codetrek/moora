{
  "name": "@moora/core",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node --format esm && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "prepublishOnly": "bun run build"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "typescript": "^5"
  }
}
